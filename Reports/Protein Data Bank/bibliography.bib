@article{westbrook_impact_2020,
	title = {Impact of the {Protein} {Data} {Bank} on antineoplastic approvals},
	volume = {25},
	issn = {1359-6446},
	url = {https://www.sciencedirect.com/science/article/pii/S1359644620300702},
	doi = {10.1016/j.drudis.2020.02.002},
	abstract = {Open access to 3D structure information from the Protein Data Bank (PDB) facilitated discovery and development of {\textgreater}90\% of the 79 new antineoplastic agents (54 small molecules, 25 biologics) with known molecular targets approved by the FDA 2010–2018. Analyses of PDB holdings, the scientific literature and related documents for each drug–target combination revealed that the impact of public-domain 3D structure data was broad and substantial, ranging from understanding target biology (∼95\% of all targets) to identifying a given target as probably druggable (∼95\% of all targets) to structure-guided lead optimization ({\textgreater}70\% of all small-molecule drugs). In addition to aggregate impact assessments, illustrative case studies are presented for three protein kinase inhibitors, an allosteric enzyme inhibitor and seven advanced-stage melanoma therapeutics.},
	language = {en},
	number = {5},
	urldate = {2022-12-02},
