	keywords = {electron microscopy, macromolecular crystallography, micro-electron diffraction, Mol*, open access, PDB, Protein Data Bank, RCSB Protein Data Bank, web-native molecular graphics, Worldwide Protein Data Bank},
	pages = {187--208},
	file = {Full Text PDF:/Users/vinaykakkar/Zotero/storage/RUHIW5GD/Burley et al. - 2022 - RCSB Protein Data Bank Celebrating 50 years of th.pdf:application/pdf;Snapshot:/Users/vinaykakkar/Zotero/storage/SEZWBVQ8/pro.html:text/html},
